___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const setDefaultConsentState = require('setDefaultConsentState');
const updateConsentState = require('updateConsentState');
const getCookieValues = require('getCookieValues');
const callInWindow = require('callInWindow');
const COOKIE_NAME = 'iw_optional_cookies_status';
const COOKIE_NAME_IDS = 'iw_allowed_cookie_ids';

const splitInput = (input) => {
  return input.split(',')
      .map(entry => entry.trim())
      .filter(entry => entry.length !== 0);
};

const arrayContains = (a, obj) => {
    for (var i = 0; i < a.length; i++) {
        if (a[i] === obj) {
            return true;
        }
    }
    return false;
};

const onUserConsent = (consentInput) => {
  const consentModeStates = {
    ad_storage: arrayContains(consentInput, 'marketing') ? 'granted' : 'denied',
    analytics_storage: arrayContains(consentInput, 'statistic') ? 'granted' : 'denied',
    functionality_storage: arrayContains(consentInput, 'always') ? 'granted' : 'denied',
  };
  updateConsentState(consentModeStates);
};

const main = (data) => {
  // defaults
  setDefaultConsentState({
    ad_storage: 'denied',
    analytics_storage: 'denied',
    functionality_storage: 'granted',
    personalization_storage: 'denied',
    security_storage: 'denied',
    wait_for_update: 500,
  });
  // from cookie
  const cookieStatus = getCookieValues(COOKIE_NAME);
  if (cookieStatus && cookieStatus[0] && (cookieStatus[0] == 'ALLOW' || cookieStatus[0] == 'SAVE')) {
    const cookieAllowedIds = getCookieValues(COOKIE_NAME_IDS);
    if (cookieAllowedIds && cookieAllowedIds[0]) {
      onUserConsent(splitInput(cookieAllowedIds[0]));
    }
  }
  // on change
  callInWindow('addIwConsentListener', 'gtm', onUserConsent);
};
main(data);
data.gtmOnSuccess();


